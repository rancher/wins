{
  "formatters": {
    "enable": [
      "gofmt"
    ],
    "exclusions": {
      "generated": "lax",
      "paths": [
        "/zz_generated_",
        "docs",
        "tests",
        "scripts",
        "charts",
        "package",
        "pkg/powershell/powershell.go",
        "suc/pkg/host/embed.go",
        "third_party$",
        "builtin$",
        "examples$"
      ]
    }
  },
  "linters": {
    "default": "none",
    "enable": [
      "govet",
      "ineffassign",
      "misspell",
      "revive"
    ],
    "exclusions": {
      "generated": "lax",
      "paths": [
        "/zz_generated_",
        "docs",
        "tests",
        "scripts",
        "charts",
        "package",
        "pkg/powershell/powershell.go",
        "suc/pkg/host/embed.go",
        "third_party$",
        "builtin$",
        "examples$"
      ],
      "presets": [
        "comments",
        "common-false-positives",
        "legacy",
        "std-error-handling"
      ]
    }
  },
  "version": "2"
}
